Package: dcTensor
Type: Package
Title: Discrete Matrix/Tensor Decomposition
Version: 0.99.0
Authors@R: c(person("Koki", "Tsuyuzaki", role = c("aut", "cre"),
                      email = "k.t.the-answer@hotmail.co.jp"))
Depends:
    R (>= 3.4.0)
Imports:
    methods,
    MASS,
    rTensor,
